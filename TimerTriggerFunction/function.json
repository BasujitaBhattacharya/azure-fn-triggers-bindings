{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */2 * * * *"
    },
    {
      "name": "inputblob",
      "type": "blob",
      "direction": "in",
      "path": "democontainer/samplefile.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}